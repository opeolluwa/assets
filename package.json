{
  "version": "1.5.0",
  "description": "A collection of web games ",
  "scripts": {
    "build-sass": "sass  src/public/stylesheets/index.scss src/public/stylesheets/index.css",
    "build-404": "sass  src/public/stylesheets/404.scss src/public/stylesheets/404.css",
    "build-ts": "tsc",
    "build-pwa": "cd user-account && npm run build && cd ..",
    "build": "bash build.sh",
    "copy-static-assets": "npx ts-node copy-static-assets.ts",
    "deploy": "git push heroku master",
    "start": "npm run serve",
    "watch-node": "nodemon src/index.ts",
    "watch-sass": "sass --watch src/public/stylesheets/index.scss src/public/stylesheets/index.css",
    "watch-404": "sass --watch src/public/stylesheets/404.scss src/public/stylesheets/404.css",
    "watch-ts": "tsc -w",
    "watch-pwa": "cd user-account && npm run serve",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,Typescript,Node,Error Page,SPA\" -c \"yellow.bold,cyan.bold,green.bold,red.bold,grey\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-node\" \"npm run watch-404\" \"npm run watch-pwa\"",
    "make": "bash install.sh"
  },
