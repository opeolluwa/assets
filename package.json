{
  "version": "22.06.12",
  "description": "A collection of web games ",
  "scripts": {
    "build-sass": "sass src/public/stylesheets/index.scss dist/public/stylesheets/index.css",
    "build-ts": "tsc",
    "build-pwa": "cd user-account && npm run build && cd ..",
    "build": "npm run build-sass &&  npm run build-ts && npm run copy-static-assets",
    "copy-static-assets": "npx ts-node copy-static-assets.ts",
    "deploy": "git push heroku master",
    "serve": "node dist/index.js",
    "start": "npm run serve",
    "watch-node": "nodemon src/index.ts",
    "watch-sass": "sass --watch src/public/stylesheets/index.scss src/public/stylesheets/index.css",
    "watch-404": "sass --watch src/public/stylesheets/404.scss src/public/stylesheets/404.css",
    "watch-ts": "tsc -w",
    "watch-pwa": "cd user-account && npm run serve",
    "dev": "concurrently -k -p \"[{name}]\" -n \"Sass,Typescript,Node,Error Page\" -c \"yellow.bold,cyan.bold,green.bold,red.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-node\" \"npm run watch-404\" ",
    "db:migrate": "npx typeorm-ts-node-esm migration:run  -d src/config/database.config.ts",
    "db:revert": "npx typeorm-ts-node-esm migration:revert -d src/config/database.config.ts"
  },
